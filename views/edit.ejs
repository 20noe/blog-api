<%- include('../partials/header') %>
<form action="/api/blogs/<%= data._id %>?_method=PUT" method="post">
    <div class="mb-3">
      <label for="title" class="form-label">Title</label>
      <input type="text" class="form-control" id="title" name="title" value="<%= data.title %>">
    </div>
    <div class="mb-3">
      <label for="text" class="form-label">Text</label>
      <input type="text" class="form-control" id="text" name="text" value="<%= data.text %>">
    </div>
    <div class="mb-3">
      <label for="confidence" class="form-label">Confidence</label>
      <input type="number" class="form-control" id="confidence" name="confidence" value="<%= data.confidence %>">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>